match(!this.data)(function() {

    this.data = {
        bundleName: this.ctx.bundleName,
        id: this.ctx.id
    };

    applyCtx({ block: 'mongo', elem: 'connect', db: 'dynamic-bem-site' });

    return Vow.when(applyCtx({ block: 'contacts' }).content).then(function(contacts) {

        return applyCtx({
            block: 'page',
            content: contacts
        });

    });

});
